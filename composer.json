{
    "name": "statamic/cms",
    "description": "Statamic CMS",
    "keywords": ["statamic", "cms", "flat file", "laravel"],
    "require": {
        "danielstjules/stringy": "dev-master",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "5.6.*",
        "laravelcollective/html": "5.6.*",
        "league/csv": "^9.0",
        "league/glide": "^1.1",
        "michelf/php-markdown": "1.7.0",
        "netcarver/textile": "3.6.*@dev",
        "symfony/yaml": "^4.1@dev",
        "webpatser/laravel-uuid": "1.*"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "dev-master",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "3.6",
        "tightenco/mailthief": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Statamic\\": "app/",
            "Statamic\\Addons\\": "bundles/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests",
            "Foo\\Bar\\": "tests/fixtures/Addon"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Statamic\\Providers\\StatamicServiceProvider"
            ],
            "aliases": {
                "Statamic": "Statamic\\Statamic"
            }
        }
    }
}
