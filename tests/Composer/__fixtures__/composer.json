        "composer/composer": "^2.2.12",
        "statamic/composer-test-example-dependency": "1.2.3"
    },
    "require-dev": {
        "statamic/composer-test-example-dev-dependency": "1.2.4"
    },
    "repositories": [
        {
            "type": "path",
            "url": "example-dependency",
            "options": {
                "symlink": false
