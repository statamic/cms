{
    "name": "@statamic/cms",
    "type": "module",
    "version": "0.0.0",
    "scripts": {
        "types": "rm -rf types && vue-tsc --declaration --emitDeclarationOnly || true"
    },
    "dependencies": {
        "@vitejs/plugin-vue": "^6.0.0"
    },
    "exports": {
        ".": {
            "types": "./types/bootstrap/cms/index.d.ts",
